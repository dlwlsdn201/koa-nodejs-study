{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
    },
    "extends": ["eslint:recommended", "prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "allowImportExportEverywhere": true,
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    },
}
